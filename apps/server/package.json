{
  "name": "rent-house-server",
  "version": "1.0.0",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "type": "module",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "lint": "bunx --bun tsc",
    "build": "bun build ./src/index.ts --outdir ./dist --target bun --format esm --external '@prisma/client'",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist && rm -f tsconfig.tsbuildinfo",
    "prisma-generate": "bunx prisma@5.12.1 generate",
    "prisma-push": "bunx prisma@5.12.1 db push --skip-generate",
    "prisma-migration": "bunx prisma@5.12.1 migrate dev --create-only --skip-seed --skip-generate",
    "prisma-studio": "bunx prisma@5.12.1 studio",
    "setup": "bun run prisma-generate"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.0.2",
    "@elysiajs/static": "^1.0.2",
    "@elysiajs/trpc": "^1.0.2",
    "@prisma/client": "^5.12.1",
    "@trpc/server": "next",
    "elysia": "^1.0.13",
    "prisma": "^5.12.1",
    "remeda": "^1.58.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bun": "^1.0.12",
    "typescript": "^5.4.5"
  }
}
