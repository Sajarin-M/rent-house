    "@prisma/client": "^5.11.0",
    "@trpc/server": "next",
    "elysia": "^1.0.10",
    "prisma": "^5.11.0",
    "remeda": "^1.57.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bun": "^1.0.12",
    "typescript": "^5.4.3"
  }
}
